{
  "name": "paramount-ui",
  "version": "1.1.1",
  "description": "React UI Components Library By WeTrust",
  "main": "node_modules/expo/AppEntry.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "paramount",
    "wetrust",
    "ui",
    "react",
    "react-native",
    "react-native-web"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build": "yarn build:es2015 && yarn build:esm && yarn build:cjs && yarn package",
    "clean": "rimraf dist",
    "develop:docs": "docz dev",
    "develop:expo": "expo start",
    "docs": "docz build",
    "generate:kitchen-sink": "node scripts/convertDoczToKitchenSink.js",
    "lint": "tslint --project ./tsconfig.json \"./src/**/*.{ts,tsx}\"",
    "package": "node scripts/packageForPublish",
    "semantic-release": "semantic-release",
    "test:snapshot": "jest -c jest.config.snapshot.js",
    "test": "yarn test:snapshot",
    "travis-deploy-once": "travis-deploy-once"
  },
  "contributors": [
    "Vu Hoang Minh (http://github.com/minheq)"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "3.2.0",
    "exenv": "1.2.2",
    "focus-trap": "5.0.1",
    "react-icons": "3.7.0",
    "react-native-svg": "9.5.1",
    "react-native-vector-icons": "6.5.0",
    "react-native-web": "0.11.4",
    "react-spring": "9.0.0-beta.1",
    "ts-essentials": "2.0.8"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@emotion/core": "10.0.10",
    "@mdx-js/mdx": "0.20.3",
    "@mdx-js/tag": "0.20.3",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.2.10",
    "@semantic-release/npm": "5.1.7",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/exenv": "1.2.0",
    "@types/jest": "24.0.13",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-native": "0.57.62",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-test-renderer": "16.8.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-preset-expo": "5.1.1",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "emotion-theming": "10.0.10",
    "expo": "33.0.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "jest-canvas-mock": "2.1.0",
    "lint-staged": "8.2.0",
    "mdast-util-to-string": "1.0.6",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-art": "16.8.6",
    "react-docgen-typescript-loader": "3.1.0",
    "react-dom": "16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-test-renderer": "16.8.6",
    "remark": "10.0.1",
    "remark-mdx": "1.0.21",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.12",
    "to-vfile": "6.0.0",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.2",
    "tslint": "5.17.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.5.1",
    "unist-util-is": "3.0.0",
    "unist-util-remove": "1.0.3",
    "unist-util-visit": "1.4.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-art": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "resolutions": {
    "terser": "3.16"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --project ./tsconfig.json",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeTrustPlatform/paramount.git"
  }
}
