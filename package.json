{
  "name": "paramount-ui",
  "version": "1.1.1",
  "description": "React UI Components Library By WeTrust",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "paramount",
    "wetrust",
    "ui",
    "react",
    "react-native",
    "react-native-web"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:es2015 && yarn build:esm && yarn build:cjs && yarn copy",
    "build:docs": "docz build",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "clean": "rimraf dist",
    "copy": "cp package.json dist/ && cp README.md dist/ && cp LICENSE dist/",
    "dev": "docz dev",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:lint": "tslint --project ./tsconfig.json \"./src/**/*.{ts,tsx}\"",
    "fix:prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "semantic-release": "semantic-release",
    "test": "jest",
    "travis-deploy-once": "travis-deploy-once"
  },
  "contributors": [
    "Vu Hoang Minh (http://github.com/minheq)"
  ],
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^8.0.0",
    "focus-trap-react": "^5.0.0",
    "react": "16.6.3",
    "react-art": "16.6.3",
    "react-dom": "16.6.3",
    "react-icons": "3.2.2",
    "react-native-web": "0.9.9",
    "react-spring": "^6.1.10"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@mdx-js/mdx": "0.16.6",
    "@semantic-release/changelog": "3.0.1",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.5",
    "@semantic-release/github": "5.2.5",
    "@semantic-release/npm": "5.1.1",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/jest": "23.3.10",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-native": "0.57.18",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "docz": "0.12.15",
    "docz-plugin-snapshots": "0.2.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "jest-canvas-mock": "1.1.0",
    "lint-staged": "8.1.0",
    "merge": "1.2.1",
    "metro-react-native-babel-preset": "0.50.0",
    "prettier": "1.15.3",
    "react-docgen-typescript-loader": "3.0.0",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.6.3",
    "rimraf": "2.6.2",
    "semantic-release": "15.12.4",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.1",
    "tslint": "5.11.0",
    "tslint-sonarts": "1.8.0",
    "typescript": "3.2.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "yarn fix:prettier",
      "git add"
    ],
    "*.{ts,tsx}": [
      "yarn fix:lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeTrustPlatform/paramount.git"
  }
}
