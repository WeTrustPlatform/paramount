{
  "name": "paramount-ui",
  "version": "1.1.1",
  "description": "React UI Components Library By WeTrust",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "paramount",
    "wetrust",
    "ui",
    "react",
    "react-native",
    "react-native-web"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:docs": "docz build",
    "build": "yarn build:es2015 && yarn build:esm && yarn build:cjs && yarn package",
    "clean": "rimraf dist",
    "dev": "start-storybook -p 6006",
    "docs": "docz dev",
    "expo": "expo start",
    "lint": "eslint . --cache --ext js,jsx,ts,tsx",
    "package": "node scripts/packageForPublish",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "contributors": [
    "Vu Hoang Minh (http://github.com/minheq)"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "4.0.0",
    "dlv": "1.1.3",
    "exenv": "1.2.2",
    "focus-trap": "5.0.2",
    "react-icons": "3.7.0",
    "react-native-svg": "9.11.1",
    "react-native-vector-icons": "6.6.0",
    "react-native-web": "0.11.5",
    "react-spring": "9.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.5.3",
    "@semantic-release/npm": "5.2.0",
    "@semantic-release/release-notes-generator": "7.3.0",
    "@storybook/react": "5.2.4",
    "@types/dlv": "1.1.1",
    "@types/exenv": "1.2.0",
    "@types/jest": "24.0.19",
    "@types/react": "16.9.6",
    "@types/react-dom": "16.9.2",
    "@types/react-native": "0.60.19",
    "@types/react-native-vector-icons": "6.4.4",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-expo": "7.1.0",
    "docz": "2.0.0-rc.55",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.15.1",
    "eslint-plugin-react-hooks": "2.1.2",
    "eslint-plugin-react-native": "3.7.0",
    "eslint-plugin-sonarjs": "0.4.0",
    "eslint-plugin-unused-imports": "0.1.0",
    "expo": "35.0.0",
    "gatsby-plugin-react-native-web": "2.0.0-beta.0",
    "husky": "3.0.9",
    "jest": "24.8.0",
    "jest-canvas-mock": "2.1.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "react": "16.10.2",
    "react-art": "16.10.2",
    "react-dom": "16.10.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-testing-library": "1.11.1",
    "react-test-renderer": "16.10.2",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.24",
    "ts-jest": "24.0.2",
    "typescript": "3.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-art": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeTrustPlatform/paramount.git"
  }
}
