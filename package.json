{
  "name": "paramount-ui",
  "version": "1.1.1",
  "description": "React UI Components Library By WeTrust",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "paramount",
    "wetrust",
    "ui",
    "react",
    "react-native",
    "react-native-web"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:docs": "docz build",
    "build": "yarn build:es2015 && yarn build:esm && yarn build:cjs && yarn package",
    "clean": "rimraf dist",
    "dev": "start-storybook -p 6006",
    "docs": "docz dev",
    "lint": "eslint . --cache --ext js,jsx,ts,tsx",
    "package": "node scripts/packageForPublish",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "contributors": [
    "Vu Hoang Minh (http://github.com/minheq)"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "4.2.2",
    "dlv": "1.1.3",
    "exenv": "1.2.2",
    "focus-trap": "5.1.0",
    "react-icons": "3.9.0",
    "react-native-svg": "12.0.3",
    "react-native-vector-icons": "6.6.0",
    "react-native-web": "0.12.2",
    "react-spring": "9.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-object-assign": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.5",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@storybook/react": "5.3.17",
    "@types/dlv": "1.1.2",
    "@types/exenv": "1.2.0",
    "@types/jest": "25.1.4",
    "@types/react": "16.9.26",
    "@types/react-dom": "16.9.5",
    "@types/react-native": "0.61.23",
    "@types/react-native-vector-icons": "6.4.5",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-jest": "25.2.3",
    "babel-loader": "8.1.0",
    "babel-plugin-react-native-web": "0.12.2",
    "docz": "2.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-react-native": "3.8.1",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unused-imports": "0.1.2",
    "gatsby-plugin-react-native-web": "2.0.0-beta.0",
    "husky": "4.2.3",
    "jest": "25.2.3",
    "jest-canvas-mock": "2.2.0",
    "lint-staged": "10.0.9",
    "prettier": "1.19.1",
    "react": "16.13.1",
    "react-native": "0.61.5",
    "react-art": "16.13.1",
    "react-dom": "16.13.1",
    "react-native-testing-library": "1.13.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.2.3",
    "ts-jest": "25.2.1",
    "typescript": "3.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-art": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeTrustPlatform/paramount.git"
  }
}
