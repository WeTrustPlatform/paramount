{
  "name": "paramount-ui",
  "version": "1.1.1",
  "description": "React UI Components Library By WeTrust",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "paramount",
    "wetrust",
    "ui",
    "react",
    "react-native",
    "react-native-web"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:es2015 && yarn build:esm && yarn build:cjs && yarn copy",
    "build:docs": "docz build",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "clean": "rimraf dist",
    "copy": "cp package.json dist/ && cp README.md dist/ && cp LICENSE dist/",
    "dev": "docz dev",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:lint": "tslint --project ./tsconfig.json \"./src/**/*.{ts,tsx}\"",
    "fix:prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "semantic-release": "semantic-release",
    "test": "jest -c jest.config.snapshot.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "contributors": [
    "Vu Hoang Minh (http://github.com/minheq)"
  ],
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "10.0.1",
    "countries-list": "2.4.2",
    "date-fns": "1.30.1",
    "deepmerge": "3.2.0",
    "exenv": "1.2.2",
    "focus-trap": "5.0.0",
    "react-icons": "3.7.0",
    "react-native-web": "0.11.2",
    "react-spring": "9.0.0-beta.1",
    "ts-essentials": "2.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@emotion/core": "10.0.10",
    "@mdx-js/mdx": "0.20.3",
    "@mdx-js/tag": "0.20.3",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.2.10",
    "@semantic-release/npm": "5.1.7",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/exenv": "1.2.0",
    "@types/expo__vector-icons": "9.0.1",
    "@types/jest": "24.0.13",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-native": "0.57.55",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "docz": "1.2.0",
    "docz-plugin-snapshots": "0.2.0",
    "docz-theme-default": "1.0.4",
    "emotion-theming": "10.0.10",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "jest-canvas-mock": "2.1.0",
    "lint-staged": "8.1.6",
    "merge": "1.2.1",
    "metro-react-native-babel-preset": "0.54.0",
    "prettier": "1.17.1",
    "react": "16.8.6",
    "react-art": "16.8.6",
    "react-docgen-typescript-loader": "3.1.0",
    "react-dom": "16.8.6",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.12",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.0",
    "tslint": "5.16.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.4.5"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-art": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "resolutions": {
    "terser": "3.16"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "yarn fix:prettier",
      "git add"
    ],
    "*.{ts,tsx}": [
      "yarn fix:lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeTrustPlatform/paramount.git"
  }
}
